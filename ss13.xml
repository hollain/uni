<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/1999/XSL/Transform">
    <xsd:element name="Lieferabrufe">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Lieferabruf" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Matrial" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Material_ID" type="xsd:integer"></xsd:element>
                                        <xsd:element name="Lieferabruf_ID" type="xsd:integer"></xsd:element>
                                        <xsd:element name="Materialbezeichnung" type="xsd:string"></xsd:element>
                                        <xsd:element name="Menge" type="xsd:integer"></xsd:element>
                                        <xsd:element name="Datum" type="xsd:integer"></xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>